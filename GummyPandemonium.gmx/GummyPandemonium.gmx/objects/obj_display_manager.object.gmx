//display properties
global.ideal_width=1024;
global.ideal_height=0;

aspect_ratio=display_get_width()/display_get_height();

global.ideal_height=round(global.ideal_width/aspect_ratio);

//Check for odd numbers
if(global.ideal_width &amp; 1) {
    global.ideal_width++;
}

//setup rooms
for(var i=1; i &lt;=room_last; i++) {
    if (room_exists(i)){
        room_set_view(i,0,true,0,0,global.ideal_width,global.ideal_height,0,0,global.ideal_width,global.ideal_height,global.ideal_width,global.ideal_height,-1,-1,obj_view_follow);
        room_set_view_enabled(i,true);
    }
}

surface_resize(application_surface,global.ideal_width,global.ideal_height);
window_set_size(global.ideal_width,global.ideal_height);
display_set_gui_size(global.ideal_width,global.ideal_height);

// Load
ini_open("saveload.ini");
if ini_read_real("vars","new",0) == 0 { // blank
    global.candy = 0;
    global.level = 1;
    global.shotgun = 0;
    global.machinegun = 0;
    global.rocketlauncher = 0;
    ini_close();
    save();
} else {
    global.candy = ini_read_real("vars", "candy", 0);
    global.level = ini_read_real("vars", "level", 1);
    global.shotgun = ini_read_real("vars", "shotgun", 0);
    global.machinegun = ini_read_real("vars", "machinegun", 0);
    global.rocketlauncher = ini_read_real("vars", "rocketlauncher", 0);
    ini_close();
    
}
global.selectedgun = 0;
room_goto(rm_menu);
